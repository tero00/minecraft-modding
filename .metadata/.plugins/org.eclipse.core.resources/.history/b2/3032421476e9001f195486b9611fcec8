package ter0minator.alchemyegg;

import net.minecraft.client.renderer.entity.RenderManager;
import net.minecraft.client.renderer.entity.RenderSnowball;
import net.minecraft.item.ItemStack;
import ter0minator.alchemyegg.EntityTransmutedEgg;

public class RenderTransmutedEgg extends RenderSnowball<EntityTransmutedEgg> {

    public RenderTransmutedEgg(RenderManager renderManager) {
        super(renderManager, Eggs.TRANSMUTED_EGG, null);
    }

    @Override
    public ItemStack getStackToRender(EntityTransmutedEgg entity) {
        return new ItemStack(Eggs.TRANSMUTED_EGG);
    }
}